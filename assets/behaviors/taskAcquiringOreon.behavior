{
    dynamic : [
        {
            guard: {
                componentPresent: "MasterOfOreon:Task",
                values: [
                    "V assignedTaskType == none"
                ],
                child: {
                    selector : [
                        {
                            timeout : {
                                time : 1,
                                child : {
                                    lookup : {
                                        tree: "Behaviors:critter"
                                    }
                                }
                            }
                        },
                        {
                            selector : [
                                look_for_task,
                                {
                                    lookup : {
                                        tree : "MasterOfOreon:advancedTasksForOreon"
                                    }
                                }
                            ]

                        }
                    ]
                }
            }
        },
        {
            sequence : [
                move_to,
                perform_task
            ]
        }
    ]
}
